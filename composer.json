{
    "name": "ai-context/symfony-ai-context-bundle",
    "description": "Symfony bundle to generate AI-readable project context.",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Guillaume",
            "email": "guillaume.valadas@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^6.0 || ^7.0",
        "symfony/finder": "^6.0 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "doctrine/orm": "^3.3",
        "doctrine/doctrine-bundle": "^2.14",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-symfony": "^2.0",
        "phpstan/extension-installer": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "AiContextBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AiContextBundle\\Tests\\": "tests/",
            "Symfony\\Component\\Security\\Http\\Attribute\\": "tests/PhpStanStubs/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
